<table summary="Book form fields">
  <tr>
    <th>Title</th>
    <td><%= f.text_field(:title) %></td>
  </tr>
  <tr>
    <th>Author</th>
    <td><%= f.text_field(:author) %></td>
  </tr>
  <tr>
    <th>Publish Date</th>
    <td><%= f.date_field(:publishDate) %></td>
  </tr>
  <tr>
    <th>Price</th>
    <td><%= f.number_field(:price, step: 'any') %></td>
  </tr>
</table>
